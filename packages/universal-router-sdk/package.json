{
  "name": "@pancakeswap/universal-router-sdk",
  "version": "1.5.7",
  "description": "sdk for integrating with the Universal Router contracts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.2",
    "@types/node-fetch": "^2.6.2",
    "chai": "^4.3.4",
    "dotenv": "^16.0.3",
    "eslint-plugin-prettier": "^3.4.0",
    "hardhat": "^2.6.8",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "tsup": "^6.7.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.3",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "@pancakeswap/sdk": "workspace:*",
    "@pancakeswap/smart-router": "workspace:*",
    "@pancakeswap/v3-sdk": "workspace:*",
    "@uniswap/permit2-sdk": "^1.2.0",
    "@uniswap/universal-router": "1.4.3",
    "bignumber.js": "^9.0.2",
    "ethers": "^5.3.1",
    "viem": "^1.9.3"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}
